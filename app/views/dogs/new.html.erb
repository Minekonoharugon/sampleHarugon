<div class="pooh">
  <h3>New Post</h3>
  <%= form_for @dog do |f| %>
    <div class="field">
      <%= f.label :body %>
      <%= f.text_field :body, :size => 100 %>
    </div>
    
    <div class="field">
      <%= f.label :image %>
      <%= f.file_field :image %>
    </div>
    <br>
    <%= f.submit "投稿する" %>
  <% end %>
  <br>
  <%= link_to "投稿一覧に戻る", dogs_path %>
</div>